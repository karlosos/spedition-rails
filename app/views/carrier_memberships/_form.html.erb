<%= simple_form_for(@carrier_membership) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :carrier_id, :collection => Carrier.all.in_any_group(@group), :label_method => :registration_number, :value_method => :id, :label => "Carrier" ,:include_blank => false %>
    <%= f.input :user_id, :as => :hidden, input_html: {value: f.object.user_id } %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
