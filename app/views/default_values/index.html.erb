<h1>Listing default_values</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @default_values.each do |default_value| %>
      <tr>
        <td><%= link_to 'Show', default_value %></td>
        <td><%= link_to 'Edit', edit_default_value_path(default_value) %></td>
        <td><%= link_to 'Destroy', default_value, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Default value', new_default_value_path %>
