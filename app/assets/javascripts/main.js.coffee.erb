jQuery ->
  pageLoad()

pageLoad = () ->
  date_picker_start()
  $('.hidden_menubar').hide()
  $('#show_hidden_menubar').click ->
    $('.hidden_menubar').toggle()

#
# Datepicker
#
date_picker_start = () ->
  $('#datetimepicker1').datepicker ->
    locale: 'pl'

  $('.date').datepicker ->
    locale: 'pl'
